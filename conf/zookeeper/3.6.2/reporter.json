{
  "system":"zookeeper",
  "servers":[
    {
      "type":"quorumpeer",
      "log":"^zookeeper-.*-server-.*.out$",
      "ready_regex":" - (FOLLOWING|LEADING)$",
      "role_regex":" - (FOLLOWING|LEADING) - LEADER ELECTION TOOK - ",
      "role_group": 1,
      "crash_regex":" - (Exiting JVM with code )"
    },
    {
      "type":"quorumpeer",
      "log":"zookeeper-.*-server-.*.out",
      "ready_regex":" - (FOLLOWING|LEADING)$",
      "role_regex":" - (FOLLOWING|LEADING) - LEADER ELECTION TOOK - ",
      "role_group": 1,
      "crash_regex":" - (Exiting JVM with code )"
    },
    {
      "type":"quorumpeer",
      "log":"zookeeper-.*-server-.*.out",
      "role_regex":" - (FOLLOWING|LEADING) - LEADER ELECTION TOOK - ",
      "role_group": 1,
      "ready_regex":" - (FOLLOWING|LEADING)$",
      "crash_regex":" - (Exiting JVM with code )"
    }
  ],
  "workload": [
    [
      {
        "type":"create",
        "number":3,
        "progress":[
          3,3,4
        ],
        "target":[
          1,2,3
        ]
      }
    ],
    [
      {
        "type":"read",
        "number":3,
        "progress":[
          40,40,40
        ],
        "target":[
          1,2,3
        ]
      },
      {
        "type":"write",
        "number":3,
        "progress":[
          40,40,40
        ],
        "target":[
          1,2,3
        ]
      }
    ]
  ],
  "cluster": [
    {
      "type":"LeaderConnector",
      "pattern":[
        {
          "trace_regex":"^org.apache.zookeeper.server.quorum.Learner\\$LeaderConnector,run,",
          "depth":6
        }
      ]
    },
    {
      "type":"Leader",
      "pattern":[
        {
          "trace_regex":"^org.apache.zookeeper.server.quorum.Leader,lead,",
          "depth":2
